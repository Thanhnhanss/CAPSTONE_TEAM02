<!DOCTYPE html>


<html xmlns="http://www.w3.org/1999/xhtml" lang="en">

<head>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;1,300;1,400;1,500;1,600;1,700;1,800&display=swap" rel="stylesheet">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/assets-ff/css/bootstrap.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/assets-ff/css/style.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/assets-ff/vendor/owl-slider.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/assets-ff/vendor/settings.css")" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/assets-ff/images/logoVLDnobri.png")" />
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/jquery-3.2.0.min.js")"></script>
    <title>@ViewBag.Title</title>
</head>

<body>
    <div class="awe-page-loading">
        <div class="awe-loading-wrapper">
            <div class="awe-loading-icon">
                <img src="@Url.Content("~/Content/assets-ff/images/logoVLDnobrw.png")" alt="LogoVLU" />
            </div>
            <div class="progress">
                <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
        </div>
    </div>

    <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
        <div class="modal-dialog modal-lg">
            <div class="modal-content popup-search">
                <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                <div class="modal-body">
                    <div class="input-group">
                        <input type="text" class="form-control control-search" placeholder="Search and hit enter...">
                        <button class="button_search" type="button">Tìm kiếm</button>
                    </div><!-- /input-group -->

                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <!-- End pushmenu -->
    <div class="wrappage">
        <!-- <div id="rtl">RTL</div> -->
        <header id="header" class="header-v3 header-v2">
            <div id="topbar" class="topbar-ver2">
                <div class="container container-ver2">
                    <div class="inner-topbar box">
                        <div class="float-left">
                            <p><img src="@Url.Content("~/Content/assets-ff/images/icon-phone-header.png.png")" alt="icon"> Gọi ngay <span> 0123456789</span></p>
                        </div>
                        <div class="float-right align-right">
                            <div class="hover-menu">
                                @using Microsoft.AspNet.Identity
                                @if (!User.Identity.IsAuthenticated)
                                {
                                    <a class="acc" href="#" title="USD"><img src="@Url.Content("~/Content/assets-ff/images/icon-user-header.png")" alt="icon">Tài Khoản</a>
                                    <ul class="list-menu" style="text-align: left; border: none; border-radius: 0.5em; padding: 0em ">
                                        <li style="margin:5px">@Html.ActionLink("Đăng ký", "Register", "../Account", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
                                        <li style="margin:5px">@Html.ActionLink("Đăng nhập", "Login", "../Account", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
                                    </ul>
                                }
                                else
                                {
                                    <a class="" href="#" title="USD">@Html.ActionLink("Xin chào! " + User.Identity.GetUserName(), "CapnhatTK", "CapnhatTK", routeValues: null, htmlAttributes: new { type = "Cập nhật thông tin" })</a>
                                    using (Html.BeginForm("LogOff", "../Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                                    {
                                        @Html.AntiForgeryToken();
                                        <ul class="list-menu" style="text-align: left; border: none; border-radius: 1em; left:20%; top:100%">
                                            <li><a href="@Url.Action("CapnhatTK", "CapnhatTK", routeValues: null)" title="Cập nhật thông tin cá nhân"><i class="fa fa-user" aria-hidden="true" style="font-size: 14px"> Cập nhật tài khoản</i></a></li>
                                            <li><a href="~/trang-chu/dang-ky-ung-tuyen" title="Ứng tuyển làm tư vấn viên"><i class="fa fa-edit" aria-hidden="true" style="font-size: 14px "> Đăng ký ứng tuyển</i></a></li>
                                            <li><a href="@Url.Action("Create", "Feedback")" title="Đánh giá và phản hồi"><i class="fa fa-comments" aria-hidden="true" style="font-size: 14px"> Đánh giá & phản hồi</i></a></li>
                                            <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-sign-out" style="font-size: 14px "> Đăng xuất</i></a></li>
                                        </ul>
                                    }
                                }

                            </div>
                            <!-- End hover-menu -->
                        </div>
                    </div>
                </div>
                <!-- End container -->
            </div>
            <!-- End topbar -->
            <div class="header-top">
                <div class="container container-ver2">
                    <div class="box">
                        <p class="icon-menu-mobile"><i class="fa fa-bars"></i></p>
                        <div class="logo">
                            <a href="~/trang-chu" title="LogoVLUDoctor">
                                <img height="58" width="145" src="@Url.Content("~/Content/assets-ff/images/logoVLDnobrr.png")" alt="images">
                            </a>
                        </div>
                        <div class="logo-mobile"><a href="/trang-chu" title="LogoVLUDoctor"><img src="@Url.Content("~/Content/assets-ff/images/logoVLDnobrw.png")" alt="LogoVLU"></a></div>

                        <div class="box">
                            <nav class="mega-menu">
                                <ul class="nav navbar-nav" id="navbar">
                                    <li class="level1"><a href="~/trang-chu" title="trang chủ">Trang chủ</a></li>
                                    <li class="level1"><a href="~/gioi-thieu" title="giới thiệu">Giới thiệu</a></li>
                                    <li class="level1"><a href="~/trang-chu/bac-si" title="danh sách bác sĩ">Bác sĩ</a></li>
                                    <li class="level1"><a href="~/trang-chu/thuoc" title="thuốc">Thuốc</a></li>
                                    <li class="level1"><a href="~/trang-chu/tin-tuc" title="tin tức">Tin Tức</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </header><!-- /header -->

        @RenderBody()
        <div id="fb-root"></div>
        <!-- Your Plugin chat code -->
        <div id="fb-customer-chat" class="fb-customerchat"></div>

        <script>
            var chatbox = document.getElementById('fb-customer-chat');
            chatbox.setAttribute("page_id", "109866731562751");
            chatbox.setAttribute("attribution", "biz_inbox");
        </script>

        <!-- Your SDK code -->
        <script>
            window.fbAsyncInit = function () {
                FB.init({
                    xfbml: true,
                    version: 'v12.0'
                });
            };

            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = 'https://connect.facebook.net/vi_VN/sdk/xfbml.customerchat.js';
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>



        <footer id="footer" class="footer-v2 align-left" style="background-image: linear-gradient(to left, #00cec880, #00cec8 ) ">

            <div class="container container-ver2">
                <div class="footer-inner">
                    <div class="row">
                        <div class="col-md-3 col-sm-5">
                            <h3 class="title-footer" style="color:white">Về chúng tôi</h3>
                            <p>Đại học Văn Lang với tuổi đời 25 năm trong ngành giáo dục, chúng tôi tự tin mang lại cho mọi người những sản phẩm mà sinh viên chúng tôi đào tạo mang lại cho mọi người.</p>
                        </div>
                        <div class="col-sm-offset-1 col-sm-3">
                            <h3 class="title-footer" style="color:white">Thông tin</h3>
                            <ul class="list-footer">
                                <li><a href="#" title="title">Giới thiệu</a></li>
                                <li><a href="#" title="title">Tìm bác sĩ</a></li>
                                <li><a href="#" title="title">Chính sách hoạt động</a></li>
                                <li><a href="#" title="title">Chính sách bảo mật</a></li>
                                <li><a href="#" title="title">Thông tin chung</a></li>
                            </ul>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <h3 class="title-footer" style="color:white">Mạng xã hội</h3>
                            <div class="social space-30">
                                <a href="#" title="t"><i class="fa fa-twitter"></i></a>
                                <a href="#" title="f"><i class="fa fa-facebook"></i></a>
                                <a href="#" title="y"><i class="fa fa-youtube-play"></i></a>
                                <a href="#" title="g"><i class="fa fa-google"></i></a>
                            </div>
                            <h3 class="title-footer" style="color:white">Đơn vị hợp tác</h3>
                            <a href="#" title="paypal"><img src="@Url.Content("~/Content/assets-ff/images/paypal-footer.png")" alt="images"></a>
                        </div>
                        <div class="col-2">
                            <ul class="float-lg-right bottom" style="z-index:1">
                                <li style="text-align:right" title="Về đầu trang">
                                    <a href='#' class="icon" style="font-size: 3em" title="Về đầu trang">🔝</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End container -->
            <div class="footer-bottom box">
                <div class="container container-ver2">

                    <div class="box bottom">
                        <p class="float-left" style="color:white">Website make by&copy;PinkPanther.</p>
                        <div class="float-right">
                            <ul class="menu-bottom-footer">
                                <li><a href="#" title="Contact Us">Giới thiệu về chúng tôi</a></li>
                                <li><a href="#" title="Term of Use">Tầm nhìn</a></li>
                                <li><a href="#" title="Privacy Policy">Chính sách bảo mật</a></li>
                                <li><a href="#" title="Site Map">Tọa lạc</a></li>
                            </ul>
                            <div class="language">
                                <span>Language:</span>
                                <div class="hover-menu">
                                    <p>VN <i class="fa fa-angle-down"></i></p>
                                    <div class="list-menu">
                                        <ul>
                                            <li><a href="#" title="EN">EN</a></li>
                                            <li><a href="#" title="VN">VN</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!-- End language -->
                        </div>
                    </div>
                </div>
                <!-- End container -->
            </div>
        </footer>
    </div>
    <!-- End wrappage -->
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/owl.carousel.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/jquery.themepunch.revolution.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/jquery.themepunch.plugins.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/engo-plugins.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/slick.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/assets-ff/js/store.js")"></script>
</body>
</html>